              </ul>
            </nav>
        
            <main>
              <h2>Current Story Catalog</h2>

              <% console.log("stories", stories.length) %>
              <% if(stories.length === 0) { %>
                  <p>No stories...yet!</p>
              <% } %> 
              <ul>
                <% stories.forEach(story => { %>
                <li>
                  <a href="/story/<%= story._id %>"> <%= story.title %> </a>
                </li>
                <% }) %>
              </ul> 
<!--         
              <# if(story.length === 0) { %>
              <p>There are no stories available yet. Check back soon or add one yourself!</p>
