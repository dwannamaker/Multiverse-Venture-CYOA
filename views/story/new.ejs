<head>
  <%- include('../partials/header') %>
    <title>Create Story</title>
  </head>
  <body class="new-story-background">
    <nav>
      <h1>Create a New Story</h1>
      <ul>
        <!-- <li>
          <a href="/user">User Contributions</a>
        </li> -->
        <li>
          <a href="/story"> <- Back to the Library </a>
        </li>
      </ul>
    </nav>
    <main>
      <form action="/story" method="POST">
        <p>
          <input type="text" name="img" placeholder="Image Here"/>
      </p>
        <p>
          <input type="text" name="title" placeholder="Title" />
        </p>
        <p>
          <input type="text" name="author" placeholder="Author" />
        </p>
        <p>
          <textarea type="text" name="body" placeholder="Body"></textarea>
        </p>


        <input type="submit" value="Create Story" />
      </form>
    </main>
  </body>
</html>
